/* default styles */
td {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 9pt;
	font-weight: normal
}

/* styles set for survey questions */
.keyWords {
	FONT-WEIGHT: bold
}

.keyPhrase {
	FONT-WEIGHT: bold;
	FONT-STYLE: italic
}

.definition {
	FONT-SIZE: 11pt;
	FONT-STYLE: italic;
	FONT-FAMILY: Arial, Helvetica, sans-serif
}

.preserve {
	white-space: pre
}

.subQ {
	font-size: 8pt
}

.keyTitle {
	FONT-WEIGHT: bold;
	FONT-SIZE: 12pt
}

.stembold {
	FONT-WEIGHT: bold;
	FONT-SIZE: 9pt
}

.itemID {
	text-align: right;
	margin-bottom: 1px;
	margin-top: 2px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 5pt;
	font-style: italic
}