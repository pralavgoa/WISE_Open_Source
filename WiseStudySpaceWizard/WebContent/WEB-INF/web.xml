<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>WiseStudySpaceWizard</display-name>
  <welcome-file-list>
    <welcome-file>LoginPage.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>submitStudySpaceParams</servlet-name>
  	<servlet-class>edu.ucla.wise.studyspacewizard.web.StudySpaceParametersAcceptor</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>submitStudySpaceParams</servlet-name>
  	<url-pattern>/submitStudySpaceParams</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>getStudySpaceParams</servlet-name>
  	<servlet-class>edu.ucla.wise.studyspacewizard.web.ParametersProviderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>getStudySpaceParams</servlet-name>
  <url-pattern>/getStudySpaceParams</url-pattern>
  </servlet-mapping>
    
  <servlet>
  	<servlet-name>destroyStudySpace</servlet-name>
  	<servlet-class>edu.ucla.wise.studyspacewizard.web.StudySpaceDestroyer</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>destroyStudySpace</servlet-name>
  <url-pattern>/destroyStudySpace</url-pattern>
  </servlet-mapping>
  
</web-app>